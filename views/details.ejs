<%- include("partials/header") -%>


<div class="row">
  <% newItems.forEach(function(newItem){ %>
      <div class="col-lg-3 col-md-4">
        <div class="card ">
          <div class="card-header">
            <h3><i class="fas fa-home wo"></i>HOME</h3>
          </div>
          <div class="card-body">
            <ul>
              <li><h2><%=newItem.name%></h2></li>
               <li><h6><%=newItem.address%></h6></li>
                <li><h4><%=newItem.number%></h4></li>
                <li><a href="/blocks/<%=newItem.number%>">Read more Details</a></li>
            </ul>
              <button type="button" class="btn btn-dark btn-block btn-lg bottom" ><%= newItem.rent %></button>
          </div>
        </div>

      </div>
  <% }) %>
<%- include("partials/footer") -%>
